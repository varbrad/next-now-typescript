{
  "version": 2,
  "name": "next-now-template",
  "builds": [
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+",
      "headers": { "cache-control": "immutable" }
    }
  ],
  "github": {
    "silent": true
  }
}
